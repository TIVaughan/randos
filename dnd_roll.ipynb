   "source": [
    "from random import randint\r\n",
    "\r\n",
    "def d(sides):\r\n",
    "    return randint(1, sides)\r\n",
    "\r\n",
    "def roll(n, sides):\r\n",
    "    return tuple(d(sides) for _ in range(n))\r\n",
    "\r\n",
    "dice = roll(2, 6)\r\n",
    "print(dice, sum(dice))\r\n"
   ],
